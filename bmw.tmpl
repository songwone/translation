}





.sharer-profile-text {
    position: absolute;
    top: 50%;
    left: .52rem;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    max-width: 2rem;
    font-family: PingFangSC-Medium;
    font-size: .17rem;
    letter-spacing: 0;
    color: #fff;
    transform: translateY(-50%);
}




.sharer-download {
    position: absolute;
    top: 50%;
    right: .17rem;
    width: .8rem;
    height: .32rem;
    transform: translateY(-50%);
}

</style>
</head>

<body>
<div id="root">
   <div class="main">

    <div class="page-wrapper">
      <div class="page-banner">
          <div class="banner-sharer">
	    <div class="sharer-content">
	      <div class="sharer-profile">
